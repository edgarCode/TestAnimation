<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    >

    <ImageView
        android:id="@+id/img"
        android:layout_width="@dimen/dp_450"
        android:layout_height="@dimen/dp_428"
        android:src="@drawable/img_qiantong"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_12"
        android:paddingLeft="@dimen/dp_80"
        android:paddingTop="@dimen/dp_20"
        />
    <ImageView
        android:id="@+id/img_qian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/qian2"
        android:layout_marginLeft="@dimen/dp_600"
        android:layout_marginTop="@dimen/dp_m_100"
        android:visibility="invisible"
        />


</RelativeLayout>
